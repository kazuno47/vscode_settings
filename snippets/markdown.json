{
	// Place your snippets for markdown here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"Print to console": {
	 	"prefix": "log",
	 	"body": [
	 		"console.log('$1');",
	 		"$2"
	 	],
	 	"description": "Log output to console"
	},

	"minutes template": {
		"prefix": "minutes",
		"body": [
			"# 日時:\n", 
			"# 場所:\n",
			"# 参加者:\n",
			"+ ",
			"+ ",
			"+ \n",
			"# アジェンダ(論点・議題):",
			"1. ",
			"2. ",
			"3. \n" ,
			"# 決まったこと:", 
			"1. ",
			"2. ",
			"3. \n",
			"# 決まらなかったこと(次に、持ち越したこと) :",
			"1. ",
			"2. ",
			"3. \n",
			"# 確認が必要なこと:", 
			"1. ",
			"2. ",
			"3. \n",
			"# 次回に向けてのTodo(誰がいつまでに) :",
			"1. ",
			"2. ",
			"3. ",
 			"<!-- 発言録は不要。補足事項として書く場合も有。 -->"
		],
		"description": "minutes template"
	},

	"table template": {
		"prefix": "<table>",
		"body": [
			"<table>",
			"\t<tr>"
			"\t\t<th></th>",
			"\t\t<th></th>",
			"\t\t<th></th>",
			"\t</tr>",
			"\t<tr>",
			"\t\t<td></td>",
			"\t\t<td></td>",
			"\t\t<td></td>",
			"\t</tr>",
			"\t<tr>",
			"\t\t<td></td>",
			"\t\t<td></td>",
			"\t\t<td></td>",
			"\t</tr>",
			"</table>"
		],
		"description": "html table tag for markdown"
	},

	"pre template": {
		"prefix": "<pre>",
		"body": [
			"<pre>",
			"$1",
			"</pre>"
		],
		"description": "pre tag for markdown"
   },

   "dl template": {
		"prefix": "<dl>",
		"body": [
			"<dl>",
				"\t<dt>$1</dt>",
				"\t<dd></dd>",
				"\t<dt></dt>",
				"\t<dd></dd>",
			"</dl>"
		],
		"description": "pre tag for markdown"
	}

}